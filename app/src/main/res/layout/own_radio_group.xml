<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/ownRadioGroupId"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical" >

    <ImageView
        android:id="@+id/moneyPaidId"
        android:layout_width="168dp"
        android:layout_height="24dp"
        app:srcCompat="@drawable/money_paid"/>

    <ImageView
        android:id="@+id/qiwiPaidId"
        android:layout_width="168dp"
        android:layout_height="24dp"
        android:layout_marginTop="12dp"
        app:srcCompat="@drawable/qiwi_paid_disable"/>

    <ImageView
        android:id="@+id/payeerPaidId"
        android:layout_width="168dp"
        android:layout_height="24dp"
        android:layout_marginTop="12dp"
        app:srcCompat="@drawable/payeer_paid_disable"/>

</LinearLayout>