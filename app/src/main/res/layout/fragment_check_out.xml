<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/checkOutFragmentId"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/main_background_light">
    <ImageView
        android:id="@+id/checkOutBackBtnId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="top|left"
        app:srcCompat="@drawable/filter_back_black_btn"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="48dp"
        android:orientation="vertical">
        <TextView
            android:id="@+id/checkOutLabelId"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/splashTextColor"
            android:layout_marginLeft="12dp"
            android:textSize="24dp"
            android:text="@string/check_out"/>

        <com.edanyma.owncomponent.CheckOutEntity
            android:id="@+id/checkOutEntityId"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="24dp"></com.edanyma.owncomponent.CheckOutEntity>
    </LinearLayout>



</FrameLayout>